<?xml version="1.0" encoding="UTF-8"?>
<Module xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/BUTR/Bannerlord.XmlSchemas/master/SubModule.xsd" >
  <Id value="$moduleid$" />
  <Name value="$modulename$" />
  <Version value="v$version$" />
  <DefaultModule value="false" />
  <ModuleCategory value="Singleplayer"/>
  <ModuleType value="Community" />
  <Url value="https://github.com/SolWayward/Bannerlord.GameMaster/" />
  <DependedModules>
    <DependedModule Id="Native" DependentVersion="v1.3.*" />
    <DependedModule Id="SandBoxCore" DependentVersion="v1.3.*" />
    <DependedModule Id="Sandbox" DependentVersion="v1.3.*" />
    <DependedModule Id="StoryMode" DependentVersion="v1.3.*" />
    <DependedModule Id="CustomBattle" DependentVersion="v1.3.*" />
  </DependedModules>
  <!-- Community Dependency Metadata -->
  <!-- https://github.com/BUTR/Bannerlord.BLSE#community-dependency-metadata -->
  <DependedModuleMetadatas>
    <DependedModuleMetadata id="Native" order="LoadBeforeThis" version="v1.3.*" />
    <DependedModuleMetadata id="SandBoxCore" order="LoadBeforeThis" version="v1.3.*" />
    <DependedModuleMetadata id="Sandbox" order="LoadBeforeThis" version="v1.3.*" />
    <DependedModuleMetadata id="StoryMode" order="LoadBeforeThis" version="v1.3.*" />
    <DependedModuleMetadata id="CustomBattle" order="LoadBeforeThis" version="v1.3.*" />
  </DependedModuleMetadatas>
  <SubModules>
    <SubModule>
      <Name value="$modulename$" />
      <DLLName value="$moduleid$.dll" />
      <SubModuleClassType value="$moduleid$.SubModule" />
      <Tags />
    </SubModule>
  </SubModules>
</Module>